{
  "required": [
    "API_KEY",
    "SERVICE_URL",
    "PORT",
    "MODE"
  ],
  "patterns": {
    "API_KEY": "^[A-Za-z0-9]{32,}$",
    "SERVICE_URL": "^https?://.*$",
    "PORT": "^\\d+$",
    "MODE": "^(dev|stage|prod)$",
    "ADMIN_EMAIL": "^[^@]+@[^@]+\\.[^@]+$"
  },
  "secret": [
    "API_KEY"
  ]
}
